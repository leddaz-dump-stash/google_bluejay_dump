#
# Bluejay sensor device configuration registry script for the dev stage.
#

# CDT information <0xPPPPSJIV>
?+0x51100 0x52100 0x52110

# LSM6DSR.
+/dev/lsm6dsr/0
bus_name=i3c0
bus_freq_hz=3800000
i3c_freq_hz=4900000
bus_addr=0x6a
i3c_addr=0xa
i3c_mrl=8
intr_id=45
trigger_type=2
fifo_enabled=1
md_threshold_m_per_s_sq=1.2264
md_window_ms=20
sd_window_ms=5000
fifo_max_odr=4000
fifo_max_wm=256
fifo_min_wm=1
gpio_id=gpio88

# LSM6DSR accel.
+/dev/lsm6dsr/0/accel
orientation_transform=-2 -1 -3

# LSM6DSR gyro.
+/dev/lsm6dsr/0/gyro
orientation_transform=-2 -1 -3

# LSM6DSR temp.
+/dev/lsm6dsr/0/temp

# ICP10101.
+/dev/icp10101/0
bus_name=i2c0
bus_freq_hz=400000
bus_addr=0x63

# ICP10101 baro.
+/dev/icp10101/0/baro

# Camera VSYNC.
# 0 = rear telephoto
+/dev/camera_vsync/0
intr_id=56
intr_trigger_type=2
# 1 = rear ultra-wide-angle
+/dev/camera_vsync/1
intr_id=57
intr_trigger_type=2
# 2 = front
+/dev/camera_vsync/2
intr_id=58
intr_trigger_type=2
# 3 = rear
+/dev/camera_vsync/3
intr_id=59
intr_trigger_type=2

# TMD3719.
+/dev/tmd3719/0
bus_name=i2c0
bus_freq_hz=400000
bus_addr=0x39
intr_id=15
sync_delay_ns=0
# falling edge = 0, rising edge = 1
te2_alignment=0
anti_aliasing=1

# TMD3719 light sensor
+/dev/tmd3719/0/als
lux_scale=1.0
c_coef=1
r_coef=-0.765091
g_coef=0.986515
b_coef=-1.221424
itime_table_brightness=96 206 316 401 2048
itime_table=1999240 1497400 998270 610360 181750
# Below itime tables are deprecated.
itime_table_30hz=1999240 1497400 998270 610360 181750
itime_table_60hz=1999240 1497400 998270 610360 181750
itime_table_90hz=1999240 1497400 998270 610360 181750
itime_table_120hz=1999240 1497400 998270 610360 181750
max_gain_table=11 10 10 10 9
fast_filter_size=10
slow_filter_size=100
aod_fast_filter_size=2
aod_slow_filter_size=2
#high_IR_threshold=0.85 # todo(b/194047112)
max_lux=160000    # todo(b/192051511)
# Number of EM cycles in a 60Hz frame.
EM_cycle_count_60Hz=4
# Step-size (in EM slots) for each rotation.
rotation_step_size=1

clear_gain10_ratio=2.0
red_gain10_ratio=2.0
green_gain10_ratio=2.0
blue_gain10_ratio=2.0

clear_gain9_ratio=4.0
red_gain9_ratio=4.0
green_gain9_ratio=4.0
blue_gain9_ratio=4.0

clear_gain8_ratio=8.0
red_gain8_ratio=8.0
green_gain8_ratio=8.0
blue_gain8_ratio=8.0

# TMD3719 proximity sensor.
+/dev/tmd3719/0/prox
auto_baseline=1
burst_delay_60hz=160
burst_delay_90hz=160
burst_delay_aod=0
hxtalk=1
pulse_count=1
vcsel_ma=10
pd0_far_threshold=100
pd0_near_threshold=100
pd1_far_threshold=80
pd1_near_threshold=120
# "Low power" operating parameters.
pulse_len_us=32
filter_len=4
skip_count=0
pdata0_scale=1.90
pdata1_scale=2.11
gain0=4
gain1=8
# "High power" operating parameters.
hp_pulse_len_us=128
hp_filter_len=1
hp_skip_count=0
hp_pdata0_scale=1.96
hp_pdata1_scale=2.32
hp_pdata0_offset=0
hp_pdata1_offset=0
hp_gain0=1
hp_gain1=2

# Display info.
+/dev/display_info/0
# Maximum panel refresh rate.
panel_max_rr=60
# Link to ALS attached to this display.
attached_alsp=/dev/tmd3719/0

# For MagCC
+/dev/magcc/0
battery_status=/sys/class/power_supply/battery/status
usb_status=/sys/class/power_supply/usb/online
usb_current=/sys/class/power_supply/usb/current_now
usb_voltage=/sys/class/power_supply/usb/voltage_now
wireless_status=/sys/class/power_supply/wireless/online
wireless_current=/sys/class/power_supply/wireless/current_now
wireless_voltage=/sys/class/power_supply/wireless/voltage_now

# MMC56X3X 0.
+/dev/mmc56x3x/0
bus_name=i3c0
bus_freq_hz=3800000
i3c_freq_hz=4900000
bus_addr=0x30
i3c_addr=0x14

# MMC56X3X 0 mag.
+/dev/mmc56x3x/0/mag
orientation_transform=2 1 -3

# Record reg_script.
+/
reg_script=bluejay_dev.reg

# FTM5.
+/dev/ftm5/0
bus_name=spi0
# TODO: (b/181889993) Run under 1MHz temporarily.
bus_freq_hz=1000000
spi_mode=0
spi_word_size=8
dchip_id_0=0x36
dchip_id_1=0x48
intr_id=17
intr_type=3
# 1 = double tap
# 2 = single tap
# 3 = long press
# 4 = single tap and long press
gesture_type=4
single_tap_min_x=92
single_tap_max_x=987
single_tap_min_y=184
single_tap_max_y=2215
single_tap_min_frame_count=3
single_tap_max_frame_count=31
single_press_motion_tolerance=255
single_press_max_touch_size=48
# LPTW v1 parameters (common)
long_press_min_x=430
long_press_max_x=650
long_press_min_y=1607
long_press_max_y=1827
long_press_min_frame_count=3
long_press_motion_tolerance=510
long_press_max_touch_size=48
# LPTW v2 parameters (extension)
long_press_extended_param=1
long_press_marginal_area_min_x=387
long_press_marginal_area_max_x=693
long_press_marginal_area_min_y=1564
long_press_marginal_area_max_y=1870
long_press_motion_tolerance_outer=530
long_press_min_touch_node_count=4
long_press_monitor_channel_min_tx=6
long_press_monitor_channel_max_tx=9
long_press_monitor_channel_min_rx=22
long_press_monitor_channel_max_rx=25

# VSC.
+/vsc
single_tap_enable=1
long_press_enable=1
quick_pick_up_enable=1

# TBN.
+/tbn
tbn_mode=1
ap2aoc_intr_id=16
ap2aoc_intr_type=4
ap2aoc_gpio_id=gpio39
aoc2ap_gpio_id=gpio90

+/vsc/auto_brightness
# float[3]={LuxToEnter, LuxToExit, RemappedLux}
lux_remap=-1.0 -1.0 -1.0

# float[3]={MinDelta, MaxDelta, LuxRatio}
lux_debounce_up=1 5000 0.15
lux_debounce_down=0.3 5000 0.1

# ALS[2], RLS[2], Fusion[2], each containing <maxSampleAgeMsec> and <maxNumOfSamples>
filter_configs=500 60 500 30 500 90
